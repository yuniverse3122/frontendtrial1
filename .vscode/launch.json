  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "pipenv",
      "args": ["run", "python", "-m", "uvicorn", "--reload", "server:app"]
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Remote Debug",
      "program": "${workspaceFolder}/server.js"
    }
  ]
}
